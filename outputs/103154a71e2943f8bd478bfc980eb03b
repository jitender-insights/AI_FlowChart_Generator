digraph Flowchart {
  rankdir=LR;
  graph [bgcolor="#f7f7f7", fontname="Arial", fontsize="12"];
  node [shape="box", style="filled", fillcolor="#e0e0e0", fontname="Arial", fontsize="10", margin="0.2,0.1", ranksep="1.5", nodesep="0.5", fontcolor="#333333", penwidth="1.5", color="#333333"];
  edge [fontname="Arial", fontsize="8", fontcolor="#666666", color="#666666"];

  subgraph cluster_input {
    style="filled";
    fillcolor="#d9f3ff";
    label="Input";
    input_tickets [label="Input Tickets"];
  }

  subgraph cluster_processing {
    style="filled";
    fillcolor="#fff0e0";
    label="Similarity Search";
    preprocess_tickets [label="Preprocess Tickets"];
    calculate_similarity [label="Calculate Similarity Scores"];
    identify_duplicates [label="Identify Duplicate Tickets"];
  }

  subgraph cluster_output {
    style="filled";
    fillcolor="#e6ffe6";
    label="Output";
    display_duplicates [label="Display Potential Duplicates"];
  }

  input_tickets -> preprocess_tickets [label="Sends"];
  preprocess_tickets -> calculate_similarity [label="Preprocessed Data"];
  calculate_similarity -> identify_duplicates [label="Similarity Scores"];
  identify_duplicates -> display_duplicates [label="Identified Duplicates"];
}
node [style=filled, fillcolor=gray20, fontcolor=white]
